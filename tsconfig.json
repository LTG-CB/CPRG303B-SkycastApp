{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react", // Or "jsx": "react" if not using React Native
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
  
}
